{
  "name": "react-static-plugin-scss-modules",
  "version": "1.0.3",
  "description": "An opinionated React-Static plugin that enables CSS Modules with SCSS syntax and PostCSS processing",
  "main": "node.api.js",
  "repository": "Cinamonas/react-static-plugin-scss-modules",
  "author": "Aleksandras Nelkinas <alex@nelkinas.dev> (https://nelkinas.dev)",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "css-loader": "^2.1.1",
    "extract-css-chunks-webpack-plugin": "^4.3.0",
    "node-sass": "^4.11.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4"
  },
  "peerDependencies": {
    "webpack": "^4.29.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add"
    ]
  }
}
